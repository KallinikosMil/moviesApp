{
  "expo": {
    "name": "CineSync",
    "slug": "cinesync",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "scheme": "cinesync",
    "assetBundlePatterns": [
      "**/*",
      "node_modules/drumber-expo-ui-lib/dist/assets/fonts/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cinesync.app",
      "android": {
        "package": "com.cinesync.app",
        "versionCode": 1
      },
      "web": {
        "favicon": "./assets/favicon.png"
      },
      "platforms": ["ios", "android", "web"],
      "description": "CineSync - A movie rating and discussion app."
    }
  }
}
